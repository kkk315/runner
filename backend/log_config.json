{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout",
      "formatter": "default"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["console"]
  },
  "loggers": {
    "uvicorn": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "uvicorn.error": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "uvicorn.access": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    }
  }
}
